<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>DoRegister</servlet-name>
        <servlet-class>com.cartoon.controller.DoRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Dologin</servlet-name>
        <servlet-class>com.cartoon.controller.Dologin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoInsert</servlet-name>
        <servlet-class>com.cartoon.controller.DoInsert</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetTypes</servlet-name>
        <servlet-class>com.cartoon.controller.GetTypes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoDelete</servlet-name>
        <servlet-class>com.cartoon.controller.DoDelete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DoRegister</servlet-name>
        <url-pattern>/register.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Dologin</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoInsert</servlet-name>
        <url-pattern>/insert.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetTypes</servlet-name>
        <url-pattern>/types.get</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoDelete</servlet-name>
        <url-pattern>/delete.do</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>